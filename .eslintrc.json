{
  "env": {
    "es2020": true,
    "node": true
  },
  "rules": {
    "no-import-assign": "off",
    "camelcase": "warn",
    "no-unused-vars": ["warn", {"argsIgnorePattern": "^_"}],
    "space-before-function-paren": ["error", "always"]
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  }
}
